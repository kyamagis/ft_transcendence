{
  "name": "transcendence-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "env-cmd -f ./.env/.env.prod nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "start:dev": "docker compose -f docker-compose.dev.yaml -p transcendence up --build",
    "start:prod": "docker compose -f docker-compose.prod.yaml -p transcendence up --build",
    "start:test": "docker compose -f docker-compose.test.yaml -p transcendence up --build",
    "down": "docker compose -f docker-compose.dev.yaml -p transcendence down; docker compose -f docker-compose.prod.yaml -p transcendence down; docker compose -f docker-compose.test.yaml -p transcendence down;",
    "type": "tsc --noEmit",
    "test": "env-cmd -f ./.env/.env.local jest",
    "test:watch": "env-cmd -f ./.env/.env.local jest --watch",
    "test:cov": "env-cmd -f ./.env/.env.local jest --coverage",
    "test:debug": "env-cmd -f ./.env/.env.local node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "migrate:dev": "env-cmd -f ./.env/.env.local prisma migrate dev --preview-feature",
    "prisma:seed": "env-cmd -f ./.env/.env.local prisma db seed --preview-feature",
    "prisma:studio": "env-cmd -f ./.env/.env.local prisma studio",
    "prisma:reset": "env-cmd -f ./.env/.env.local prisma migrate reset --preview-feature"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.1.1",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^10.0.1",
    "@nestjs/platform-express": "^10.2.5",
    "@nestjs/platform-socket.io": "^10.2.4",
    "@nestjs/websockets": "^10.2.4",
    "@prisma/client": "^5.1.1",
    "axios": "^1.5.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "connect-pg-simple": "^9.0.0",
    "express-session": "^1.17.3",
    "passport": "^0.6.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "passport-oauth2": "^1.7.0",
    "pg": "^8.11.3",
    "qrcode": "^1.5.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "socket.io": "^4.7.2",
    "speakeasy": "^2.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.17.7",
    "@types/jest": "^29.5.2",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.3.1",
    "@types/passport-jwt": "^3.0.9",
    "@types/qrcode": "^1.5.2",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "env-cmd": "^10.1.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "prisma": "^5.1.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "overrides": {
    "oauth": "~0.10.0"
  }
}
